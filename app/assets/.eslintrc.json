{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": ["airbnb"],
  "rules": {
    "space-before-function-paren": ["error", "never"],
    "func-names": ["error", "never"],
    "spaced-comment": [
      "error",
      "always",
      {
        "markers": ["="]
      }
    ],
    "lines-around-comment": [
      "warn",
      {
        "beforeLineComment": false
      }
    ],
    "max-len": ["error", { "code": 120 }]
  }
}
